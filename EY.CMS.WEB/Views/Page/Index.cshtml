@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@model EY.CMS.CORE.DTOs.PageDto

<div class="se-pre-con"></div>

<div class="about-area overflow-hidden shape-box default-padding">
    <div class="container">
        <div class="about-box">
            <div class="row align-center">
                <div class="col-lg-12 thumb-left">
                    @if (ViewBag.Culture == "en-US")
                    {
                         @Html.Raw(Model.TextEN)
                    }
                    else if (ViewBag.Culture == "ru-RU")
                    {
                         @Html.Raw(Model.TextRU)
                    }
                    else
                    {
                        @Html.Raw(Model.Text)
                    }
                </div>

            </div>
        </div>
    </div>
</div>


